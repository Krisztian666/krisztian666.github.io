  display: flex;
  align-items: flex-end;
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  pointer-events: none;
  z-index: 0;
} 
  z-index: 10;//{speak:"Modify z-index.",position:10,delete:[11]}

